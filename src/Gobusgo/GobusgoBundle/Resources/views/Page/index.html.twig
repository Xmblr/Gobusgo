{% block body %}

    <div class="container" style="padding:0px;">
        <div class="row">
            <h1 style="margin: 0% 0% 5% 0%;">Куда везем груз?</h1>
            <div class="col-md-4" style="margin-bottom:20px;">
                <div class="background-round">
                    <h3 class="reload" style="display:table-cell !important; vertical-align: middle; " OnClick="a();">
                        Москва</br>Минск</h3>
                </div>
            </div>
            <div class="col-md-4" style="margin-bottom:20px;">
                <div class="background-round">
                    <h3 style="display:table-cell !important; vertical-align: middle; ">Минск</h3>
                </div>
            </div>
            <div class="col-md-4" style="margin-bottom:20px;">
                <div class="background-round">
                    <h3 style="display:table-cell !important; vertical-align: middle; ">Беларусь</h3>
                </div>
            </div>
            <script language="JavaScript">
                function sleep(ms) {
                    return new Promise(resolve => setTimeout(resolve, ms));
                }

                async function a(e, w, audio) {
                    audio = new Audio(); // Создаём новый элемент Audio
                    audio.src = 'audio/car.mp3'; // Указываем путь к звуку "клика"
                    audio.autoplay = true; // Автоматически запускаем
                    e = document.getElementById("car");
                    w = document.getElementById("wheel1");
                    s = document.getElementById("wheel2");

                    e.style.webkitAnimationName = "shake";
                    w.style.webkitAnimationName = "shake-w";
                    s.style.webkitAnimationName = "shake-w";

                    await sleep(1500);

                    e.style.transform = "translate(2000px,0px)";
                    e.style.transition = "all 5s ease-in-out";

                    w.style.transform = "translate(2000px,0px) rotate(5turn)";
                    w.style.transition = "all 5s ease-in-out";
                    s.style.transform = "translate(2000px,0px) rotate(10turn)";
                    s.style.transition = "all 5s ease-in-out";
                }
            </script>
        </div>
    </div>
    <div class="container">
        <div class="row">
            <div class="car-gobusgo">
                <img src="images/car-body.png" id="car" width="200px;" style="position: absolute; padding-top: 53px;">
