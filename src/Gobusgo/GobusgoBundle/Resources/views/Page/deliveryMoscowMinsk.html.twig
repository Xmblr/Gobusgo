            <div class="col-md-3 col-sm-3 col-xs-3 road-tablet">
                <div style="height: 123px;">
                    <h3 class="road-tablet-title">Клиент</h3>
                </div>

                <div style="height: 123px;">
                    <h3 class="road-tablet-title">Габариты</h3>
                </div>
                <div style="height: 123px;">
                    <h3 class="road-tablet-title">Город</h3>
                </div>
                <div style="height: 123px;">
                    <h3 class="road-tablet-title">ИТОГО</h3>
                </div>
                <div style="height: 123px;">
                    <h3 class="road-tablet-title">Заявка</h3>
                </div>
            </div>
            <div class="col-md-2 col-sm-2 col-xs-2 road-tablet" style="text-align:center;">
                <img src="{{ asset('images/road-tablet.png') }}" width="60px">
            </div>
            <div class="col-xs-3 road-mobile" style="padding: 0px 5px;">
                <img src="{{ asset('images/road-tablet.png') }}" width="60px">
            </div>
                <div class="col-md-12 col-sm-7 col-xs-9">
                    <div class="col-md-3 height-mobile">
                        <h4 style="text-align: center;">Клиент</h4>
                        <label style="display: block;">
                            <span>Физическое лицо</span>
                            <input type="radio" class="option-input radio" name="loptop" id="individual" onchange="sum()"
                                   checked="checked"/>
                        </label>
                        <label style="display: block;">
                            <span>Юридическое лицо</span>
                            <input type="radio" class="option-input radio" name="loptop" id="legal" onchange="sum()"/>

                        </label>

                    </div>
                    <div class="col-md-3 height-mobile">

                        <h4 style="text-align: center;">Вес (кг.)</h4>
                        <div class="Input">
                            <input class="input-log" type="text" id="weight" name="_username" value="" required="required"
                                   placeholder="Вес, кг" onchange="sum()">
                        </div>
                    </div>
                    <div class="col-md-3 height-mobile">
                        <h4 style="text-align: center;">Направление</h4>
                        <p class="big">Москва <i class="fa fa-long-arrow-right" aria-hidden="true"></i> Минск</p>
                    </div>

