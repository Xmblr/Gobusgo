{% block body %}
    <div class="container offset-top-22 offset-md-top-40">
        <div class="row">
            <h1>{{ seo.h1|raw }}</h1>
            {% if seo.intro is not empty %} {{ seo.intro|raw }} {% endif %}
        </div>
    </div>
