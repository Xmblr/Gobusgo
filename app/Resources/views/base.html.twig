<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />

        {% block stylesheets %}
            {{ include('@TwitterBootstrap/html5shiv.html.twig') }}
            <meta name="viewport" content="width=device-width, initial-scale=1.0">
            {{ twitter_assets('stylesheets') }}
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
    <div>
        {% block header %}
            <h2 style="color: green">Header from base.html.twig</h2>
        {% endblock %}
    </div>
    <div class="container">
        {% block body %}

        {% endblock %}
    </div>
    <div>
        {% block footer %}
            <footer><p>&copy; New-Line 2018</p></footer>
        {% endblock %}
    </div>
        {% block javascripts %}
            {{ twitter_assets('javascripts') }}
        {% endblock %}
    </body>
</html>
