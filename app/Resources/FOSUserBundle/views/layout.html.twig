{#{% extends '@GobusgoGobusgo/layout.html.twig' %}#}
{% extends '::base.html.twig' %}

{% block header %}
    <div class="container-fluid">
        <div class="row" style="text-align: left;">
            <a href="{{ path('gobusgo_gobusgo_homepage') }}"><img class="exit-img"
                                                                  src="{{ asset('/images/home.png') }}"></a>
            <a onclick="javascript:history.back(); return false;"> <img class="exit-img"
                                                                        src="{{ asset('/images/return.png') }}">
            </a>


        </div>
    </div>

{% endblock %}

{% block body %}

    <body>
    <div class="page">

        <section class="section-40">
            <div class="container">
                <div class="row">
                    {% if app.request.hasPreviousSession %}
                        {% for type, messages in app.session.flashbag.all() %}
                            {% for message in messages %}
                                <h4 class="flash-{{ type }}" style="text-align: center;">
                                    {{ message }}
                                </h4>
                            {% endfor %}
                        {% endfor %}
                    {% endif %}

                    <div class="col-md-12">
                        {% block fos_user_content %}
                        {% endblock fos_user_content %}
                    </div>
                </div>
            </div>
        </section>
    </div>
    </body>

{% endblock %}

{% block footer %}

{% endblock %}
